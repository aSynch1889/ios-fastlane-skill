# Copy this file to fastlane/.env.fastlane or fastlane/.env.fastlane.ci and update values.

# Required for upload
PGYER_API_KEY=YOUR_PGYER_API_KEY

# Optional match setup (recommended for team/CI)
MATCH_GIT_URL={{MATCH_GIT_URL}}
MATCH_GIT_BRANCH={{MATCH_GIT_BRANCH}}
# MATCH_PASSWORD=xxx

# Quality gate switches
ENABLE_QUALITY_GATE={{ENABLE_QUALITY_GATE}}
ENABLE_TESTS={{ENABLE_TESTS}}
ENABLE_SWIFTLINT={{ENABLE_SWIFTLINT}}

# Optional target override
# FL_TARGET={{PROJECT_NAME}}
